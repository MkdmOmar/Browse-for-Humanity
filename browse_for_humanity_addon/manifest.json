{
  "name": "Browse for Humanity",
  "description": "Lend your computing power to save people accross the world",
  "version": "1",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
	"background": {
    "page": "background.html"
  },
  "browser_action": {
     "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "webNavigation",
    "webRequestBlocking",
    "tabs",
    "http://*/*", "https://*/*",
    "alarms",
    "notifications" 
  ]
}
